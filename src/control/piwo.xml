<?xml version="1.0"?>
<!DOCTYPE productDefines SYSTEM "/usr/share/YaST2/control/control.dtd">
<productDefines  xmlns="http://www.suse.com/1.0/yast2ns"
    xmlns:config="http://www.suse.com/1.0/configns">

    <!--
    Work around for the text domain
    textdomain="piwo"
    -->

    <textdomain>piwo</textdomain>

    <workflows  config:type="list">
        <workflow>
            <defaults>
                <enable_back>yes</enable_back>
                <enable_next>yes</enable_next>
                <archs>all</archs>
            </defaults>
            <stage>normal</stage>
            <label>Finish Installation</label>
            <mode>normal</mode>
            <modules  config:type="list">
                <module>
                    <label>Registration</label>
                    <name>inst_suse_register</name>
                    <enable_back>no</enable_back>
                </module>
                <module>
                    <label>Software Repositories</label>
                    <name>inst_productsources</name>
                    <enable_back>yes</enable_back>
                    <enable_next>yes</enable_next>
                </module>
                <module>
                    <label>Online Update</label>
                    <name>online_update</name>
                    <enable_back>yes</enable_back>
                    <enable_next>yes</enable_next>
                </module>
            </modules>
        </workflow>
    </workflows>
</productDefines>
